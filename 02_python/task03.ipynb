    "principal = int(input(\"元金を入力してください: \"))\n",
    "rate = int(input(\"年利を入力してください: \"))\n",
    "years = 10\n",
    "\n",
    "price = principal\n",
    "for i in range(years):\n",
    "    price = price * ( 1 + (rate / 100) )\n",
    "\n",
    "print(f\"{years}年後の金額: {round(price, 2)}\")\n"
   ]
