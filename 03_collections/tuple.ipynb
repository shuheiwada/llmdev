   "source": [
    "numbers = (0, 1, 2, 3, 4, 5)\n",
    "\n",
    "# インデックス1から3の要素を取得（3は含まない）\n",
    "print(numbers[1:3])  # (1, 2)\n",
    "\n",
    "# 最初からインデックス3まで（3は含まない）\n",
    "print(numbers[:3])  # (0, 1, 2)\n",
    "\n",
    "# インデックス2から最後まで\n",
    "print(numbers[2:])  # (2, 3, 4, 5)\n",
    "\n",
    "# 全要素を逆順に\n",
    "print(numbers[::-1])  # (5, 4, 3, 2, 1, 0)"
   ]
  }
